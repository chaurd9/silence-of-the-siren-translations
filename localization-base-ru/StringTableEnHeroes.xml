            <!--Exploration skills-->
            <GameDBLocalizedString>         <LocID>HERO_SKILL_EXPLORER</LocID>                              <Text>Explorer</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>HERO_SKILL_EXPLORER_DESCRIPTION</LocID>                  <Text><![CDATA[Permanently adds <color=SOTS_COLOR_MOVEMENT>4 Movement Points</color> with each skill level.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_EXPLORER1</LocID>                           <Text><![CDATA[Permanently adds <color=SOTS_COLOR_MOVEMENT>4 Movement Points</color>.\nMeeting any friendly commander restores <color=SOTS_COLOR_MOVEMENT>2 Movement Points</color> once per phase.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_EXPLORER2</LocID>                           <Text><![CDATA[Permanently adds <color=SOTS_COLOR_MOVEMENT>8 Movement Points</color>.\nMeeting any friendly commander restores <color=SOTS_COLOR_MOVEMENT>4 Movement Points</color> once per phase.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_EXPLORER3</LocID>                           <Text><![CDATA[Permanently adds <color=SOTS_COLOR_MOVEMENT>12 Movement Points</color>.\nMeeting any friendly commander restores <color=SOTS_COLOR_MOVEMENT>6 Movement Points</color> once per phase.]]></Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>HERO_SKILL_ENGINEER</LocID>                              <Text>Engineer</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>HERO_SKILL_ENGINEER_DESCRIPTION</LocID>                  <Text><![CDATA[Adds a <color=SOTS_COLOR_ENERGY>5 Energy Point</color> daily recharge with each skill level, and upgrades other aspects of energy use.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_ENGINEER1</LocID>                           <Text><![CDATA[Recharges <color=SOTS_COLOR_ENERGY>5 Energy Points</color> daily.\nEquipment drains <color=SOTS_COLOR_ENERGY>10%</color> less <color=SOTS_COLOR_ENERGY>energy</color>.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_ENGINEER2</LocID>                           <Text><![CDATA[Recharges <color=SOTS_COLOR_ENERGY>10 Energy Points</color> daily.\nArtifacts drain <color=SOTS_COLOR_ENERGY>10%</color> less <color=SOTS_COLOR_ENERGY>energy</color>.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_ENGINEER3</LocID>                           <Text><![CDATA[Recharges <color=SOTS_COLOR_ENERGY>15 energy points</color> daily.\nAdds <color=SOTS_COLOR_ENERGY>30 Energy Points</color> to commander max <color=SOTS_COLOR_ENERGY>energy</color>.]]></Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>HERO_SKILL_NEGOTIATOR</LocID>                            <Text>Negotiator</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>HERO_SKILL_NEGOTIATOR_DESCRIPTION</LocID>                <Text>Some units from each stack in the neutral army will join your commander before the battle. (Based on skill level and army arrangement.)</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_NEGOTIATOR1</LocID>                         <Text>10%, or at least 1 unit, from each unit stack will join.\n(For 80% of their original price.)</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_NEGOTIATOR2</LocID>                         <Text>20%, or at least 1 unit, from each unit stack will join.\n(For 60% of their original price.)</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_NEGOTIATOR3</LocID>                         <Text><![CDATA[30%, or at least 1 unit, from each unit stack will join.\nRemoves <color=SOTS_COLOR_GOLD>Morale</color> penalty for non-faction units in the army.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_NEGOTIATOR3_TOOLTIP</LocID>                 <Text>(Negotiator on level 3 cancels penalties for non-faction units in the army)</Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>HERO_SKILL_BUILDER</LocID>                               <Text>Builder</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>HERO_SKILL_BUILDER_DESCRIPTION</LocID>                   <Text><![CDATA[Structures on the world map get constructed in their <color=SOTS_COLOR_GOLD>upgraded variant</color>. Level 3 adds <color=SOTS_COLOR_GOLD>Cybersnare</color> to the available structures.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_CHEAPER_PRODUCTION_UPGRADES</LocID>         <Text><![CDATA[Production upgrades of buildings on the map cost <color=SOTS_COLOR_GOLD>{1}%</color> less.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_CHEAPER_GARRISON_UPGRADES</LocID>           <Text><![CDATA[Garrison upgrades of buildings on the world map are <color=SOTS_COLOR_GOLD>free</color>!]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_BUILDER_DISCOUNT_PERCENT</LocID>            <Text><![CDATA[Building structures is discounted by <color=SOTS_COLOR_GOLD>{1}%</color>!]]> </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_TRACKER_DISCOUNT_PERCENT</LocID>            <Text><![CDATA[Breaking Xenoslimes costs <color=SOTS_COLOR_GOLD>{1}%</color> less!]]> </Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>HERO_SKILL_SCOUT</LocID>                                 <Text>Scout</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>HERO_SKILL_SCOUT_DESCRIPTION</LocID>                     <Text><![CDATA[Increases visibility range, provides more info about hostile armies and predicts locations of undiscovered treasures. Commander is unaffected by traps and environment hazards.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_SCOUT1</LocID>                              <Text><![CDATA[<color=SOTS_COLOR_GOLD>Visibility Range +1</color>.\n<color=SOTS_COLOR_GOLD>Adds a (?) icon</color> for treasures and resources 5 tiles into the FOW.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_SCOUT2</LocID>                              <Text><![CDATA[<color=SOTS_COLOR_GOLD>Visibility range +2</color>.\n<color=SOTS_COLOR_GOLD>Adds a (?) icon</color> for mines and other conquerable structures 5 tiles into the FOW.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_SCOUT3</LocID>                              <Text><![CDATA[<color=SOTS_COLOR_GOLD>Visibility range +3</color>.\n<color=SOTS_COLOR_GOLD>Adds a (?) icon</color> for equipment, artifacts, bases and beneficial structures 5 tiles into the FOW.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_SPY1</LocID>                                <Text><![CDATA[Displays enemy commander's <color=SOTS_COLOR_GOLD>Attributes</color>.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_SPY2</LocID>                                <Text><![CDATA[Displays enemy commander's <color=SOTS_COLOR_GOLD>Attributes and Skills</color>.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_SPY3</LocID>                                <Text><![CDATA[Displays the enemy commander's <color=SOTS_COLOR_GOLD>Attributes and Skills</color>. Also shows the <color=SOTS_COLOR_GOLD>enemy army</color>.]]></Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>HERO_SKILL_COLLECTOR</LocID>                             <Text>Collector</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>HERO_SKILL_COLLECTOR_DESCRIPTION</LocID>                 <Text>Adds an extra slot for artifacts or equipment with each skill level. Allows picking up items from dangerous zones around neutral armies without triggering a battle. </Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_COLLECTOR1</LocID>                          <Text>+1 equipment slot.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_COLLECTOR2</LocID>                          <Text><![CDATA[+2 equipment slots.\n +10% Chance to gain additional primal <color=SOTS_COLOR_FUSION_CORE>Fusion core</color>.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_COLLECTOR3</LocID>                          <Text><![CDATA[+1 artifact slot.\nPlayer can see a (?) icon for <color=SOTS_COLOR_FUSION_CORE>Fusion cores</color> 5 tiles into in the FOW.]]></Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>HERO_SKILL_TRACKER</LocID>                               <Text>Tracker</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>HERO_SKILL_TRACKER_DESCRIPTION</LocID>                   <Text><![CDATA[Reduces penalty for moving through rough terrain. Shows trails of your enemies. The commander is ignored by Primal turrets and other defenses.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_TERRAIN_COST_REDUCED</LocID>                <Text><![CDATA[<color=SOTS_COLOR_GOLD>Penalty for rough terrain</color> is reduced by {1}%.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_SEE_ENEMY_PATHS_TILES</LocID>               <Text><![CDATA[Can see tracks of last {1} steps of <color=SOTS_COLOR_GOLD>enemy commanders</color>.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_EXTRA_MOVE_POINTS_FROM_CONSUMABLES</LocID>  <Text><![CDATA[Consumables restore {1}% more <color=SOTS_COLOR_MOVEMENT>movement</color>.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_SCOUTING_BONUS</LocID>                      <Text>Scouting range +{1} tiles.</Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>HERO_SKILL_EXPLORATION_ABILITIES</LocID>                 <Text>Globetrotter</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>HERO_SKILL_EXPLORATION_ABILITIES_DESCRIPTION</LocID>     <Text><![CDATA[Learning or leveling up Globetrotter lets you learn a variety of <color=SOTS_COLOR_GOLD>Exploration Abilities</color>. After learning this skill, visit a base with an <color=SOTS_COLOR_GOLD>Academy</color> to learn more abilities.]]></Text>  </GameDBLocalizedString>

            <!--Combat skills-->
            <GameDBLocalizedString>         <LocID>HERO_SKILL_BRAWLER</LocID>                               <Text>Brawler</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>HERO_SKILL_BRAWLER_DESCRIPTION</LocID>                   <Text>Improves your units' melee attacks.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_MELEE_DAMAGE_BONUS</LocID>                  <Text><![CDATA[Melee attacks do <color=SOTS_COLOR_RED>+{1}% damage</color>]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_MELEE_UNIT_MOVEMENT_BONUS</LocID>           <Text>Melee units: movement in combat +{1}</Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>HERO_SKILL_SNIPER</LocID>                                <Text>Sniper</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>HERO_SKILL_SNIPER_DESCRIPTION</LocID>                    <Text><![CDATA[Improves your units' ranged <color=SOTS_COLOR_RED>Damage</color>.]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_RANGED_DAMAGE_BONUS</LocID>                 <Text><![CDATA[Ranged attacks do <color=SOTS_COLOR_RED>+{1}% damage</color>]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_LONG_RANGE_PENALTY_REDUCTION</LocID>        <Text><![CDATA[Ranged attacks outside of range do<color=SOTS_COLOR_RED> {1}% more damage</color>]]></Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>HERO_SKILL_DEFENSIVE_SPECIALIST</LocID>                  <Text>Defensive Specialist</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>HERO_SKILL_DEFENSIVE_SPECIALIST_DESCRIPTION</LocID>      <Text>Improves your units' defense and gives them a chance to retaliate against multiple attacks in a single round.</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_DEFENSE_BOOST</LocID>                       <Text><![CDATA[Increases the <color=SOTS_COLOR_UPGRADE>Defense</color> attribute of your units by <color=SOTS_COLOR_UPGRADE>{1}</color>]]></Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>SKILL_EFFECT_SECOND_RETALIATION_CHANCE</LocID>           <Text>Units have a {1}% chance to retaliate against two attacks in a single round</Text>  </GameDBLocalizedString>

            <GameDBLocalizedString>         <LocID>HERO_SKILL_CHESS_MASTER</LocID>                          <Text>Chess Master</Text>  </GameDBLocalizedString>
            <GameDBLocalizedString>         <LocID>HERO_SKILL_CHESS_MASTER_DESCRIPTION</LocID>              <Text>Allows you to position this commander's army at the beginning of a battle.</Text>  </GameDBLocalizedString>
